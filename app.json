{
  "expo": {
    "name": "Calygam",
    "scheme": "Calygam",
    "slug": "Calygam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Image/LogoCalygam.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Image/LogoCalygam.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Image/adaptive-icon.png",
        "backgroundColor": "#00000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mr_cat.Calygam"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "a9bd4283-889c-4997-a058-2698fcbf4905"
      }
    }
  }
}
